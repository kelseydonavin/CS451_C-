<Window x:Class="CS451_Milestone1.CheckIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CS451_Milestone1"
        mc:Ignorable="d"
        Title="Check In" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="Check_In" HorizontalAlignment="Left" Height="340" Margin="54,45,0,0" VerticalAlignment="Top" Width="678">
            <DataGrid.Columns>
                <DataGridTextColumn x:Name="Name" Header="Name"/>
                <DataGridTextColumn x:Name="Month" Header="Month"/>
                <DataGridTextColumn x:Name="Day" Header="Day"/>
                <DataGridTextColumn x:Name="Year" Header="Year"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
